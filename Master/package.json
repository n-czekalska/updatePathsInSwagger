    "js-yaml": "3.9.1",
    "rxjs": "6.3.3",
    "typescript": "2.9.2"
  },
  "devDependencies": {
    "@angular/cli": "16.0.0",
    "@royallondon/angular-tslint-configuration": "1.0.0",
    "@types/fs-extra": "5.0.4",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0"
  }
